-----简单的设置cookie
    设置cookie
    url=reverse('index')
    re=redirect(url)
    re.set_cookie('username', username)
    获取cookie
    request.COOKIES.get('username')
-----js中使用cookie
    设置cookie参数同上
    $.cookie('the_cookie', 'the_value', { expires: 7, path: '/' });
    获取cookie
    $.cookie('the_cookie')
-----cookie的参数
键值对
key
value
--设置超时是时间
max_age
--设置超时截止时间
expires
--设置生效路径
path='/'
--设置生效域名
domain
--https传输
secure=False
--设置js不能获取
httponly=False
-----加密的cookie
创建cookie
url=reverse('users')
re=redirect(url)
re.set_signed_cookie('username','1','abcabc')
获取cookie
cookie=request.get_signed_cookie('username','abcabc')